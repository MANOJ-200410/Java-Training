import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int c = 0;
        if(n==0){
            System.out.println("1");
        } else
        {
            
        
        while(n!=0){
            c++;
            n = n/10;
        }
        System.out.println(c);
        }
        
    }
}
