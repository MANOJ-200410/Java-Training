import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        if(a%400==0)
            System.out.println("Leap Year");
        else if(a%100!=0 && a%4==0)
            System.out.println("Leap Year");
        else
             System.out.println("Not a Leap Year");
       
           
        
        
    }
}
