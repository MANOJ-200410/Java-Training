import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        System.out.println("Total teams formed: " + (a/b));
        
        System.out.println("Players left out: " + (a%b));
        
    }
}
